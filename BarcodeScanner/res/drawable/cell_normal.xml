<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:left="2dp"
        android:right="2dp">
        <shape android:shape="rectangle" >
            <gradient
                android:angle="270"
                android:endColor="#ebf2e8"
                android:startColor="#ebf2e8" />

            <stroke
                android:dashGap="1dp"
                android:dashWidth="5dp"
                android:width="1dp"
                android:color="#0f0" />
            <corners android:radius="8dp" />
        </shape>
    </item>
</layer-list>